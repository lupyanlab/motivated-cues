@article{YuvalGreenberg:2008gt,
author = {Yuval-Greenberg, Shlomit and Deouell, Leon Y},
title = {{The dog's meow: asymmetrical interaction in cross-modal object recognition}},
journal = {Experimental Brain Research},
year = {2008},
volume = {193},
number = {4},
pages = {603--614},
month = dec,
annote = {Methods: Attend either the visual or auditory modality, and answer a prime following the presentation of the stimuli ( e.g., 'dog?' ) to which the participants responded yes or no.
Exp. 1 Results: When attending the audio ( e.g. 'dog [bark]?' ), there was a larger benefit for visual congruency than when attending the visual ( e.g., 'dog [picture]?' ). However, when the pictures were reduced in contrast, this effect went away - making a reliable interaction. There was no interaction with delay, suggesting that the difference is not due to time of processing constraints.
Exp. 2 Results: General replication of Exp. 1 with the prime following the presentation of the stimuli now presented auditory ( rather than visually ).

General discussion: Information in an unattended modality can facilitate processing in an attended modality. The interaction with the effects of contrast reveal that priority is placed on the most informative ( least noisy ) cues rather than some rigid modality-specific hierarchy.

[ von Kriegstein {\&}amp; Giraud (2006) - 'natural and ecologically valid' congruency effects ]
[ Molholm et al. (2004) - cross modal congruency effects ]
[ Greene et al., (2001) - auditory events prime only auditory events?? ]
}
}

